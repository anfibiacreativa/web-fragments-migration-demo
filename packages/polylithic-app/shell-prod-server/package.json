{
  "name": "@demongbe/prod-server",
  "version": "1.0.0",
  "description": "Prod server for the angular app bootstrapping the fragments",
  "main": "dist/app.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start:server": "node dist/app.js",
    "dev": "ts-node-dev --respawn src/app.ts",
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "web-fragments": "^0.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "@types/express": "^4.17.17",
    "typescript": "^5.2.2",
    "ts-node-dev": "^2.0.0"
  },
  "keywords": [
    "prod-server",
    "web-fragments-shell"
  ],
  "author": "Natalia Venditto",
  "license": "MIT"
}
